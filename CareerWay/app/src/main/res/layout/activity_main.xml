<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#FFF5EE"
    tools:context=".MainActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">



        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10sp"
            android:layout_marginLeft="10sp"
            android:layout_marginRight="10sp">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:padding="10sp"
                    android:src="@drawable/h1"

                    />

                <TextView
                    android:layout_width="160sp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:paddingTop="30sp"
                    android:text="KnowWay"
                    android:textAlignment="center"
                    android:textColor="#000"
                    android:textSize="30sp"
                    android:textStyle="bold|italic" />

                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:padding="10sp"
                    android:src="@drawable/h2"

                    />


            </LinearLayout>


        </androidx.cardview.widget.CardView>


        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10sp"
            android:rowCount="2"
            android:columnCount="2"
            android:layout_marginRight="10sp"
            android:layout_marginLeft="10sp"

            >

            <androidx.cardview.widget.CardView
                android:id="@+id/class10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:layout_gravity="fill"
                android:layout_margin="10sp"

                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10sp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:paddingLeft="60sp"
                    android:paddingTop="30sp"
                    android:paddingBottom="30sp"
                    android:text="Class10th"
                    android:textAlignment="center"
                    android:textColor="#000"

                    android:textSize="16sp" />

                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="100dp"
                    android:padding="10sp"

                    android:src="@drawable/class10">

                </ImageView>


            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
android:id="@+id/class12th"
                android:layout_gravity="fill"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10sp"
                android:layout_margin="10sp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Class12th"
                    android:paddingLeft="80sp"
                    android:textSize="14sp"
                    android:textColor="#000"
                    android:textAlignment="center"
                    android:layout_gravity="center"
                    android:paddingTop="20sp"
                    android:paddingBottom="20sp"

                    />

                <ImageView

                    android:layout_width="100dp"
                    android:layout_height="80dp"
                    android:padding="10sp"
                    android:paddingTop="40sp"
                    android:src="@drawable/class12">

                </ImageView>





            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/Btech"
                android:layout_gravity="fill"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10sp"
                android:layout_margin="10sp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Btech"
                    android:textSize="14sp"
                    android:paddingLeft="50sp"
                    android:textColor="#000"
                    android:textAlignment="center"
                    android:layout_gravity="center"
                    android:paddingTop="20sp"
                    android:paddingBottom="20sp"

                    />


                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="100dp"
                    android:padding="10sp"
                    android:src="@drawable/mtech">

                </ImageView>



            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/Mtech"
                android:layout_gravity="fill"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10sp"
                android:layout_margin="10sp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mtech"
                    android:textSize="14sp"
                    android:textColor="#000"
                    android:textAlignment="center"
                    android:layout_gravity="center"
                    android:paddingTop="20sp"
                    android:paddingLeft="50sp"
                    android:paddingBottom="20sp"
                    />


                <ImageView
                    android:layout_width="80dp"
                    android:layout_height="100dp"
                    android:padding="10sp"
                    android:src="@drawable/btech">

                </ImageView>




            </androidx.cardview.widget.CardView>

